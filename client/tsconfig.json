{
  "compilerOptions": {
    "baseUrl": "./src/",
    "paths": {
      "@modules/*": ["modules/*"],
      "@utilities/*": ["utilities/*"],

    },
    "outDir": "./dist/",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strict": true,
    "module": "es6",
    "target": "es5",
    "allowJs": true,
    "moduleResolution": "node"
  }
}